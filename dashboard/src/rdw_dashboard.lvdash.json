{
  "datasets": [
    {
      "name": "c7707718",
      "displayName": "registered_vehicles",
      "queryLines": [
        "SELECT * FROM gold_catalog.rdw_etl.registered_vehicles"
      ]
    },
    {
      "name": "top20_brands",
      "displayName": "top20_brands",
      "queryLines": [
        "SELECT make, COUNT(*) as count FROM gold_catalog.rdw_etl.registered_vehicles GROUP BY make ORDER BY count DESC LIMIT 20"
      ]
    },
    {
      "name": "job_runs",
      "displayName": "job_runs",
      "queryLines": [
        "WITH jobs AS ( ",
        "  SELECT *, ROW_NUMBER() OVER (PARTITION BY workspace_id, job_id ORDER BY change_time DESC) as rn ",
        "  FROM system.lakeflow.jobs ",
        "  WHERE (name LIKE '%RDW%' OR name LIKE '%Bronze%' OR name LIKE '%Silver%' OR name LIKE '%Gold%')",
        "  QUALIFY rn = 1 ",
        ") ",
        "SELECT ",
        "  jrt.run_id, ",
        "  jobs.name as job_name, ",
        "  jrt.result_state, ",
        "  jrt.trigger_type, ",
        "  jrt.period_start_time, ",
        "  jrt.period_end_time, ",
        "  COALESCE(TIMESTAMPDIFF(SECOND, jrt.period_start_time, jrt.period_end_time), 0) as duration_seconds, ",
        "  ROUND(COALESCE(TIMESTAMPDIFF(SECOND, jrt.period_start_time, jrt.period_end_time), 0) / 60.0, 1) as duration_minutes ",
        "FROM system.lakeflow.job_run_timeline jrt ",
        "LEFT JOIN jobs USING (workspace_id, job_id) ",
        "WHERE jobs.name IS NOT NULL ",
        "  AND jrt.result_state IS NOT NULL ",
        "  AND jrt.period_start_time > CURRENT_TIMESTAMP() - INTERVAL 30 DAYS"
      ]
    },
    {
      "name": "job_stats",
      "displayName": "job_stats",
      "queryLines": [
        "WITH jobs AS ( ",
        "  SELECT *, ROW_NUMBER() OVER (PARTITION BY workspace_id, job_id ORDER BY change_time DESC) as rn ",
        "  FROM system.lakeflow.jobs ",
        "  WHERE (name LIKE '%RDW%' OR name LIKE '%Bronze%' OR name LIKE '%Silver%' OR name LIKE '%Gold%')",
        "  QUALIFY rn = 1 ",
        "), ",
        "job_runs AS ( ",
        "  SELECT ",
        "    jobs.name as job_name, ",
        "    jrt.run_id, ",
        "    jrt.result_state, ",
        "    SUM(TIMESTAMPDIFF(SECOND, jrt.period_start_time, jrt.period_end_time)) as duration_seconds ",
        "  FROM system.lakeflow.job_run_timeline jrt ",
        "  LEFT JOIN jobs USING (workspace_id, job_id) ",
        "  WHERE jobs.name IS NOT NULL ",
        "    AND jrt.result_state IS NOT NULL ",
        "    AND jrt.period_start_time > CURRENT_TIMESTAMP() - INTERVAL 30 DAYS ",
        "  GROUP BY jobs.name, jrt.run_id, jrt.result_state ",
        ") ",
        "SELECT ",
        "  job_name, ",
        "  COUNT(*) as total_runs, ",
        "  SUM(CASE WHEN result_state = 'SUCCEEDED' THEN 1 ELSE 0 END) as successful_runs, ",
        "  SUM(CASE WHEN result_state IN ('FAILED', 'ERROR', 'TIMED_OUT') THEN 1 ELSE 0 END) as failed_runs, ",
        "  ROUND(100.0 * SUM(CASE WHEN result_state = 'SUCCEEDED' THEN 1 ELSE 0 END) / COUNT(*), 1) as success_rate, ",
        "  ROUND(AVG(duration_seconds) / 60.0, 1) as avg_duration_minutes ",
        "FROM job_runs ",
        "GROUP BY job_name ",
        "ORDER BY job_name"
      ]
    },
    {
      "name": "pipeline_layer_counts",
      "displayName": "pipeline_layer_counts",
      "queryLines": [
        "SELECT ",
        "  'Bronze' as layer, ",
        "  'registered_vehicles' as table_name, ",
        "  COUNT(*) as row_count ",
        "FROM bronze_catalog.rdw_etl.registered_vehicles ",
        "UNION ALL ",
        "SELECT ",
        "  'Silver' as layer, ",
        "  'registered_vehicles' as table_name, ",
        "  COUNT(*) as row_count ",
        "FROM silver_catalog.rdw_etl.registered_vehicles ",
        "WHERE __is_current = true ",
        "UNION ALL ",
        "SELECT ",
        "  'Gold' as layer, ",
        "  'registered_vehicles' as table_name, ",
        "  COUNT(*) as row_count ",
        "FROM gold_catalog.rdw_etl.registered_vehicles"
      ]
    },
    {
      "name": "pipeline_freshness",
      "displayName": "pipeline_freshness",
      "queryLines": [
        "SELECT ",
        "  'registered_vehicles' as table_name, ",
        "  MAX(__processed_time) as last_processed, ",
        "  ROUND(TIMESTAMPDIFF(HOUR, MAX(__processed_time), CURRENT_TIMESTAMP()), 1) as hours_since_update ",
        "FROM silver_catalog.rdw_etl.registered_vehicles ",
        "UNION ALL ",
        "SELECT ",
        "  'registered_vehicles_fuel' as table_name, ",
        "  MAX(__processed_time) as last_processed, ",
        "  ROUND(TIMESTAMPDIFF(HOUR, MAX(__processed_time), CURRENT_TIMESTAMP()), 1) as hours_since_update ",
        "FROM silver_catalog.rdw_etl.registered_vehicles_fuel ",
        "UNION ALL ",
        "SELECT ",
        "  'registered_vehicles_axles' as table_name, ",
        "  MAX(__processed_time) as last_processed, ",
        "  ROUND(TIMESTAMPDIFF(HOUR, MAX(__processed_time), CURRENT_TIMESTAMP()), 1) as hours_since_update ",
        "FROM silver_catalog.rdw_etl.registered_vehicles_axles"
      ]
    },
    {
      "name": "scd2_operations",
      "displayName": "scd2_operations",
      "queryLines": [
        "SELECT ",
        "  __operation as operation, ",
        "  COUNT(*) as record_count ",
        "FROM silver_catalog.rdw_etl.registered_vehicles ",
        "WHERE __processed_time > CURRENT_TIMESTAMP() - INTERVAL 7 DAYS ",
        "GROUP BY __operation ",
        "ORDER BY record_count DESC"
      ]
    },
    {
      "name": "quarantine_counts",
      "displayName": "quarantine_counts",
      "queryLines": [
        "SELECT ",
        "  table_name, ",
        "  1 as quarantined_count ",
        "FROM bronze_catalog.information_schema.tables ",
        "WHERE table_schema = 'rdw_etl_quarantine' ",
        "ORDER BY table_name"
      ]
    }
  ],
