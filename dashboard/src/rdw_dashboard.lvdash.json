      ]
    },
    {
      "name": "job_runs",
      "displayName": "job_runs",
      "queryLines": [
        "WITH jobs AS ( ",
        "  SELECT *, ROW_NUMBER() OVER (PARTITION BY workspace_id, job_id ORDER BY change_time DESC) as rn ",
        "  FROM system.lakeflow.jobs ",
        "  WHERE (name LIKE '%RDW%' OR name LIKE '%Bronze%' OR name LIKE '%Silver%' OR name LIKE '%Gold%')",
        "  QUALIFY rn = 1 ",
        ") ",
        "SELECT ",
        "  jrt.run_id, ",
        "  jobs.name as job_name, ",
        "  jrt.result_state, ",
        "  jrt.trigger_type, ",
        "  jrt.period_start_time, ",
        "  jrt.period_end_time, ",
        "  COALESCE(TIMESTAMPDIFF(SECOND, jrt.period_start_time, jrt.period_end_time), 0) as duration_seconds, ",
        "  ROUND(COALESCE(TIMESTAMPDIFF(SECOND, jrt.period_start_time, jrt.period_end_time), 0) / 60.0, 1) as duration_minutes ",
        "FROM system.lakeflow.job_run_timeline jrt ",
        "LEFT JOIN jobs USING (workspace_id, job_id) ",
        "WHERE jobs.name IS NOT NULL ",
        "  AND jrt.result_state IS NOT NULL ",
        "  AND jrt.period_start_time > CURRENT_TIMESTAMP() - INTERVAL 30 DAYS"
      ]
    },
    {
      "name": "job_runs_by_state",
      "displayName": "job_runs_by_state",
      "queryLines": [
        "WITH jobs AS ( ",
        "  SELECT *, ROW_NUMBER() OVER (PARTITION BY workspace_id, job_id ORDER BY change_time DESC) as rn ",
        "  FROM system.lakeflow.jobs ",
        "  WHERE (name LIKE '%RDW%' OR name LIKE '%Bronze%' OR name LIKE '%Silver%' OR name LIKE '%Gold%')",
        "  QUALIFY rn = 1 ",
        ") ",
        "SELECT ",
        "  jobs.name as job_name, ",
        "  jrt.result_state, ",
        "  COUNT(DISTINCT jrt.run_id) as run_count ",
        "FROM system.lakeflow.job_run_timeline jrt ",
        "LEFT JOIN jobs USING (workspace_id, job_id) ",
        "WHERE jobs.name IS NOT NULL ",
        "  AND jrt.result_state IS NOT NULL ",
        "  AND jrt.period_start_time > CURRENT_TIMESTAMP() - INTERVAL 30 DAYS ",
        "GROUP BY jobs.name, jrt.result_state ",
        "ORDER BY jobs.name, run_count DESC"
      ]
    },
    {
      "name": "job_runs_daily",
      "displayName": "job_runs_daily",
      "queryLines": [
        "WITH jobs AS ( ",
        "  SELECT *, ROW_NUMBER() OVER (PARTITION BY workspace_id, job_id ORDER BY change_time DESC) as rn ",
        "  FROM system.lakeflow.jobs ",
        "  WHERE (name LIKE '%RDW%' OR name LIKE '%Bronze%' OR name LIKE '%Silver%' OR name LIKE '%Gold%')",
        "  QUALIFY rn = 1 ",
        ") ",
        "SELECT ",
        "  DATE(jrt.period_start_time) as run_date, ",
        "  jrt.result_state, ",
        "  COUNT(DISTINCT jrt.run_id) as run_count ",
        "FROM system.lakeflow.job_run_timeline jrt ",
        "LEFT JOIN jobs USING (workspace_id, job_id) ",
        "WHERE jobs.name IS NOT NULL ",
        "  AND jrt.result_state IS NOT NULL ",
        "  AND jrt.period_start_time > CURRENT_TIMESTAMP() - INTERVAL 30 DAYS ",
        "GROUP BY DATE(jrt.period_start_time), jrt.result_state ",
        "ORDER BY run_date"
      ]
    },
    {
      "name": "job_stats",
      "displayName": "job_stats",
      "queryLines": [
        "WITH jobs AS ( ",
        "  SELECT *, ROW_NUMBER() OVER (PARTITION BY workspace_id, job_id ORDER BY change_time DESC) as rn ",
        "  FROM system.lakeflow.jobs ",
        "  WHERE (name LIKE '%RDW%' OR name LIKE '%Bronze%' OR name LIKE '%Silver%' OR name LIKE '%Gold%')",
        "  QUALIFY rn = 1 ",
        "), ",
        "job_runs AS ( ",
        "  SELECT ",
        "    jobs.name as job_name, ",
        "    jrt.run_id, ",
        "    jrt.result_state, ",
        "    SUM(TIMESTAMPDIFF(SECOND, jrt.period_start_time, jrt.period_end_time)) as duration_seconds ",
        "  FROM system.lakeflow.job_run_timeline jrt ",
        "  LEFT JOIN jobs USING (workspace_id, job_id) ",
        "  WHERE jobs.name IS NOT NULL ",
        "    AND jrt.result_state IS NOT NULL ",
        "    AND jrt.period_start_time > CURRENT_TIMESTAMP() - INTERVAL 30 DAYS ",
        "  GROUP BY jobs.name, jrt.run_id, jrt.result_state ",
        ") ",
        "SELECT ",
        "  job_name, ",
        "  COUNT(*) as total_runs, ",
        "  SUM(CASE WHEN result_state = 'SUCCEEDED' THEN 1 ELSE 0 END) as successful_runs, ",
        "  SUM(CASE WHEN result_state IN ('FAILED', 'ERROR', 'TIMED_OUT') THEN 1 ELSE 0 END) as failed_runs, ",
        "  ROUND(100.0 * SUM(CASE WHEN result_state = 'SUCCEEDED' THEN 1 ELSE 0 END) / COUNT(*), 1) as success_rate, ",
        "  ROUND(AVG(duration_seconds) / 60.0, 1) as avg_duration_minutes ",
        "FROM job_runs ",
        "GROUP BY job_name ",
        "ORDER BY job_name"
      ]
    },
    {
      "name": "recent_job_runs",
      "displayName": "recent_job_runs",
      "queryLines": [
        "WITH jobs AS ( ",
        "  SELECT *, ROW_NUMBER() OVER (PARTITION BY workspace_id, job_id ORDER BY change_time DESC) as rn ",
        "  FROM system.lakeflow.jobs ",
        "  WHERE (name LIKE '%RDW%' OR name LIKE '%Bronze%' OR name LIKE '%Silver%' OR name LIKE '%Gold%')",
        "  QUALIFY rn = 1 ",
        ") ",
        "SELECT ",
        "  jobs.name as job_name, ",
        "  jrt.run_id, ",
        "  jrt.result_state, ",
        "  jrt.trigger_type, ",
        "  jrt.period_start_time as start_time, ",
        "  jrt.period_end_time as end_time, ",
        "  ROUND(SUM(TIMESTAMPDIFF(SECOND, jrt.period_start_time, jrt.period_end_time)) / 60.0, 1) as duration_minutes ",
        "FROM system.lakeflow.job_run_timeline jrt ",
        "LEFT JOIN jobs USING (workspace_id, job_id) ",
        "WHERE jobs.name IS NOT NULL ",
        "  AND jrt.result_state IS NOT NULL ",
        "  AND jrt.period_start_time > CURRENT_TIMESTAMP() - INTERVAL 7 DAYS ",
        "GROUP BY jobs.name, jrt.run_id, jrt.result_state, jrt.trigger_type, jrt.period_start_time, jrt.period_end_time ",
        "ORDER BY jrt.period_start_time DESC ",
        "LIMIT 50"
      ]
    }
  ],
